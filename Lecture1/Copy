# LIST
copy_list = original_list[:]

# TUPLE
copy_tuple = tuple(original_tuple)

# SET
copy_set = set(original_set)

# DICTIONARY
copy_dict = original_dict.copy() #OF dict(original_dict)


# STACK (meestal list)
stack_copy = original_stack[:]

# LINKED LIST (algemene singly linked list)
copy_ll = LinkedList()
cur = LL._LinkedList__head
while cur:
    copy_ll.add_task(cur.task, cur.duration, cur.priority)
    cur = cur.next


# QUEUE (list implementatie!)
copy = queue[:] 

# QUEUE (LINKED list implementatie!) 
copy_q = Queue()
cur = Q._Queue__head
while cur:
    copy_q.enqueue(cur.element)
    cur = cur.next


# PRIORITY QUEUE (heapq + SpecialSorted)
import copy
copy_pq = PriorityQueue(sortkey=PQ.sortkey)
